<Application x:Class="AutoPartsStore.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoPartsStore"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="lanquage" Source="View\Resources\DictionaryDefault.xaml"/>
                <ResourceDictionary x:Name="theme" Source="View\Resources\DictionaryWhite.xaml"/>
                <ResourceDictionary x:Name="lanquage" Source="View\Resources\RusDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
